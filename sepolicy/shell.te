# address denials with some CTS cases
allow shell persist_file:dir r_dir_perms;
allow shell sysfs_battery_supply:dir r_dir_perms;
allow shell sysfs_usb_supply:dir r_dir_perms;

allow shell sysfs_battery_supply:file read;
allow shell sysfs_battery_supply:lnk_file read;
allow shell sysfs_usb_supply:file read;
allow shell sysfs_usb_supply:lnk_file read;

